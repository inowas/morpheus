  "scripts": {
    "analyze-morpheus": "source-map-explorer 'dist/morpheus/*.js'",
    "start-morpheus": "webpack serve --hot --config webpack.config.morpheus.ts --mode development --env mockserver --open",
    "start-morpheus-integration-local": "webpack serve --hot --config webpack.config.morpheus.ts --mode development --env BASE_API_URL=http://localhost:5000 --env KEYCLOAK_URL=http://identity.inowas.localhost --env KEYCLOAK_REALM=inowas --env KEYCLOAK_CLIENT_ID=morpheus-frontend --open",
    "start-mfviz": "webpack serve --config webpack.config.mfviz.ts --hot --mode development --open",
    "start-simpletools": "webpack serve --config webpack.config.simpletools.ts --hot --mode development --open",
    "start-storybook": "storybook dev -p 6006",
    "build-morpheus": "webpack --config webpack.config.morpheus.ts --mode production --env BASE_API_URL=https://api.dev.morpheus.inowas.com --env KEYCLOAK_URL=https://identity.inowas.com --env KEYCLOAK_REALM=inowas-dev --env KEYCLOAK_CLIENT_ID=morpheus-frontend --stats-children",
    "build-mfviz": "webpack --config webpack.config.mfviz.ts --mode production --stats-children",
    "build-simpletools": "webpack --config webpack.config.simpletools.ts --mode production --stats-children",
    "build-storybook": "storybook build -o dist/storybook",
    "test": "jest --testPathIgnorePatterns=.*e2e.test.ts --watch",
    "test:ci": "jest --testPathIgnorePatterns=.*e2e.test.ts",
    "test:coverage": "jest --testPathIgnorePatterns=.*e2e.test.ts --coverage",
    "test:e2e": "jest -c jest.e2e.config.ts",
    "cs-check": "eslint --quiet --ext .js,.jsx,.ts,.tsx .",
    "cs-fix": "eslint --quiet --fix --ext .js,.jsx,.ts,.tsx .",
    "depcruise:app": "dependency-cruiser src/app --include-only '^(src/app)' --config",
    "depcruise:app:graph": "dependency-cruiser src/app --include-only '^(src/app/modules)' --focus ^src/app/modules --config --output-type dot | dot -T svg | tee dependency-graph.svg | depcruise-wrap-stream-in-html > dependency-graph.html",
    "depcruise:app:baseline": "dependency-cruiser src/app --config  -T baseline -f .dependency-cruiser-known-violations.json",
    "depcruise:app:explain": "dependency-cruiser src/app --config --output-type err-long --progress none",
    "depcruise:all": "dependency-cruiser src --config"
  }
